#include<iostream>
#include<vector>


int main() {

  int num{1};
  int i{0};
   int min{0};
   int min_index{0};
  std::vector<int> totalN;
  std::cout << "Please enter some integers terminated by 0" << std::endl;

  while(num != 0) {
    std::cin >> num;
    totalN.push_back (num);
    i++;
    }
 
   for(int k{0}; k < i - 1; k++) {
     min = totalN[k];
     for(int j{k}; j < i - 1; j++) {
	if( min > totalN[j]) {
	  min = totalN[j];
	  min_index = j;
	}
      }
     if(min == totalN[k]) {
       totalN[k] = totalN[k];
     }
    else {
	totalN[min_index] = totalN[k];
	totalN[k] = min;
    }
   }
   for ( int m{0}; m < i - 1; m ++)
     std::cout << totalN[m] << std::endl;
   
  return 0;
}
